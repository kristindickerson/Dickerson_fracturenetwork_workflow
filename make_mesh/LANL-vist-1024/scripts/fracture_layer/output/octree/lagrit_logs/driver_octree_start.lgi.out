define/XMIN/-4999.000000000000                                                  
define/YMAX/4999.000000000000                                                   
define/YMIN/-4999.000000000000                                                  
define/ZMAX/4999.000000000000                                                   
define/ZMIN/-4999.000000000000                                                  
